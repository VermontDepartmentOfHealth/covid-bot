{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch 11ty",
      "program": "${workspaceFolder}/node_modules/@11ty/eleventy/cmd.js",
      "args": []
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Fetch KB",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\fetchKB.js",
      "args": [
        "--environment",
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Restore KB",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\restoreKB.js",
      "args": [
        "--environment",
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Publish KB",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\_index.js",
      "args": [
        "--",
        "publish-kb",
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy KB",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\deployKB.js",
      "args": [
        "--source",
        "test",
        "--target",
        "prod"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Archive Data",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\_index",
      "args": [
        "--",
        "archive-data"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Lint Data",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\_index.js",
      "args": [
        "--",
        "lint-data"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "List Changes",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\_index.js",
      "args": [
        "--",
        "list-changes"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Fix Data",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\fixFollowUpsOnLocalFaq.js",     
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Publish FAQ",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\cli\\publishFaq.js",
      "args": [
        "--target",
        "\\\\vdhwebapps-test\\C$\\VDHApplications\\COVID\\faq\\"
      ]
    }
  ]
}
