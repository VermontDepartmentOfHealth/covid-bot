			"item": [
				{
					"name": "Generate Answer",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "EndpointKey {{RuntimeApiKey}}"
							},
							{
								"key": "Content-type",
								"value": "application/json"
							}
