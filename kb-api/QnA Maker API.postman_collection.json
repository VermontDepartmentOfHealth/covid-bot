			"item": [
				{
					"name": "Generate Answer",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "EndpointKey {{RuntimeApiKey}}"
							},
							{
								"key": "Content-type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\"question\":\"How many windows in NY\"}"
						},
						"url": {
							"raw": "{{RuntimeEndpoint}}/qnamaker/knowledgebases/{{kbId}}/generateAnswer",
							"host": [
								"{{RuntimeEndpoint}}"
							],
							"path": [
